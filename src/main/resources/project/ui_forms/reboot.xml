<editor>
    <help>/commander/pages/@PLUGIN_NAME@/@PLUGIN_KEY@_help?s=Administration&amp;ss=Plugins#Reboot</help>
    <formElement>
        <label>Configuration:</label>
        <property>connection_config</property>
        <value/>
        <documentation>The name of the config which holds all the connection information. This must reference a valid existing configuration.</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
    <formElement>
        <label>Tenant ID:</label>
        <property>tenant_id</property>
        <value/>
        <documentation>ID of the tenant to use.</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
    <formElement>
        <label>Server ID:</label>
        <property>server_id</property>
        <value/>
        <documentation>ID of the Server to reboot.</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
    <formElement>
        <label>Reboot Type:</label>
        <property>reboot_type</property>
        <value/>
        <documentation>Reboot type (Hard/Soft).</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>         
</editor>