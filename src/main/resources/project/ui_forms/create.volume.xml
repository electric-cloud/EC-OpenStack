<editor>
    <help>/commander/pages/@PLUGIN_NAME@/@PLUGIN_KEY@_help?s=Administration&amp;ss=Plugins#CreateVolume</help>
    <formElement>
        <label>Configuration:</label>
        <property>connection_config</property>
        <value/>
        <documentation>The name of the config which holds all the connection information. This must reference a valid existing configuration.</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
    <formElement>
        <label>Tenant ID:</label>
        <property>tenant_id</property>
        <value/>
        <documentation>ID of the tenant to use.</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
    <formElement>
        <label>Display Name</label>
        <property>display_name</property>
        <value/>
        <documentation>Name of the volume to display</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
    <formElement>
        <label>Size</label>
        <property>size</property>
        <value/>
        <documentation>Size of the volume</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
    <formElement>
        <label>Volume Type</label>
        <property>volume_type</property>
        <value/>
        <documentation>Type of the volume to create</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
    <formElement>
        <label>Availability Zone</label>
        <property>availability_zone</property>
        <value/>
        <documentation>Availability Zone of the volume</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
</editor>
