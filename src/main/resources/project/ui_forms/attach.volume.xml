<editor>
    <help>/commander/pages/@PLUGIN_NAME@/@PLUGIN_KEY@_help?s=Administration&amp;ss=Plugins#CreateVolume</help>
    <formElement>
        <label>Configuration:</label>
        <property>connection_config</property>
        <value/>
        <documentation>The name of the config which holds all the connection information. This must reference a valid existing configuration.</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
    <formElement>
        <label>Tenant ID:</label>
        <property>tenant_id</property>
        <value/>
        <documentation>ID of the tenant to use.</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
    <formElement>
        <label>Volume Id</label>
        <property>volume_id</property>
        <value/>
        <documentation>Name of the volume to attach</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
    <formElement>
        <label>Server Id</label>
        <property>server_id</property>
        <value/>
        <documentation>UUID of the server</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
    <formElement>
        <label>Device</label>
        <property>device</property>
        <value>auto</value>
        <documentation>Name of the device</documentation>
        <required>1</required>
        <type>entry</type>
    </formElement>
</editor>
